name = "SimpleSAML Authentication for CUL"
description = "Preconfigured authentication for CUL websites using SimpleSAML."
core = "6.x"
package = "Features"
version = "6.x-1.0-rc1"
project = "simplesaml_authentication_for_cul"
dependencies[] = "features"
dependencies[] = "simplesamlphp_auth"
dependencies[] = "strongarm"
features[ctools][] = "strongarm:strongarm:1"
features[user_permission][] = "administer simpleSAMLphp authentication"
features[variable][] = "simplesamlphp_auth_activate"
features[variable][] = "simplesamlphp_auth_allowdefaultlogin"
features[variable][] = "simplesamlphp_auth_allowdefaultloginusers"
features[variable][] = "simplesamlphp_auth_authsource"
features[variable][] = "simplesamlphp_auth_forcehttps"
features[variable][] = "simplesamlphp_auth_installdir"
features[variable][] = "simplesamlphp_auth_mailattr"
features[variable][] = "simplesamlphp_auth_registerusers"
features[variable][] = "simplesamlphp_auth_roleevaleverytime"
features[variable][] = "simplesamlphp_auth_unique_id"
features[variable][] = "simplesamlphp_auth_user_name"
features[variable][] = "simplesamlphp_auth_user_register_original"
project status url = "https://featureserver.library.cornell.edu/fserver"
